@article{chen2019MetapopulationModelPathogen,
  title = {Metapopulation {{Model}} from {{Pathogen}}’s {{Perspective}}: {{A Versatile Framework}} to {{Quantify Pathogen Transfer}} and {{Circulation}} between {{Environment}} and {{Hosts}}},
  shorttitle = {Metapopulation {{Model}} from {{Pathogen}}’s {{Perspective}}},
  author = {Chen, Shi and Lanzas, Cristina and Lee, Chihoon and Zenarosa, Gabriel L. and Arif, Ahmed A. and Dulin, Michael},
  date = {2019-02-08},
  journaltitle = {Scientific Reports},
