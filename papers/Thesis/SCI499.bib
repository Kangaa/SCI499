@article{sartorius2021ModellingPredictingSpatiotemporal,
  title = {Modelling and Predicting the Spatio-Temporal Spread of {{COVID-19}}, Associated Deaths and Impact of Key Risk Factors in {{England}}},
  author = {Sartorius, B. and Lawson, A. B. and Pullan, R. L.},
  date = {2021-03-08},
  journaltitle = {Scientific Reports},
